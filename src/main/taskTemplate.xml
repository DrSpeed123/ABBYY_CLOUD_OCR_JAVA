        <text id="numberField" bottom="0" left="0" right="0" top="0">
             <language>Russian</language>
			 <letterSet>1234567890.</letterSet>
             <rotation>clockwise</rotation>
             <regExp>([0-9][0-9])\s([0-9][0-9])\s([0-9][0-9][0-9][0-9][0-9][0-9])</regExp>
			 <oneTextLine>true</oneTextLine>
        </text>
